{
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "name": "websequencediagrams",
  "description": "Interface to websequencediagrams.com",
  "version": "1.0.0",
  "homepage": "https://github.com/hildjj/node-websequencediagrams",
  "repository": {
    "type": "git",
    "url": "git://github.com/hildjj/node-websequencediagrams"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "main": "lib/wsd",
  "types": "types/wsd.d.ts",
  "engines": {
    "node": ">=12.19"
  },
  "keywords": [
    "uml",
    "sequence diagram",
    "sequence",
    "diagram",
    "websequencediagrams",
    "websequencediagrams.com"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "test": "ava",
    "lint": "eslint . --ext js --ext cjs --ext mjs --ext md bin/*",
    "coverage": "nyc -r lcov npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "precoveralls": "npm run coverage",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "^0.0.8",
    "@types/node": "^16.4.13",
    "ava": "^3.15.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-node": "^11.1.0",
    "nock": "^13.1.1",
    "nyc": "^15.1.0",
    "tmp-promise": "^3.0.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "optimist": "~0.6"
  },
  "standard": {
    "ignore": [
      "docs/"
    ],
    "envs": [
      "node"
    ]
